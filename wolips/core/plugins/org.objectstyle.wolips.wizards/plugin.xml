<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>


<!-- =================================================================================== -->
<!-- Extension: Project Wizards                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="WOLips"
            id="org.objectstyle.wolips.wizards">
      </category>
      <wizard
            name="WOComponent"
            icon="icons/wobuilder/WOComponentBundle.png"
            category="org.objectstyle.wolips.wizards"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.component">
            <class class="org.objectstyle.wolips.wizards.WOComponentCreationWizard">
              <parameter name="javatype" value="true"/>
            </class>
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="WOComponent Controller/View"
            icon="icons/action/new-component-controller.png"
